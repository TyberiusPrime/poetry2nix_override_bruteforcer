n":null},{"comment_text":"","digests":{"blake2b_256":"f787489f642d16248b974191670c11deb340c9a4a19f51719e70c5b1c09ff269","md5":"a3ee42041ff447d013b938522231a7d8","sha256":"8cd341c4ee26437272718976eeeea82ce8e98f2286d730d65c4b2380d67d805f"},"downloads":-1,"filename":"Products.ContentWellPortlets-2.0.1.tar.gz","has_sig":false,"md5_digest":"a3ee42041ff447d013b938522231a7d8","packagetype":"sdist","python_version":"source","requires_python":null,"size":18875,"upload_time":"2009-10-13T16:53:40","upload_time_iso_8601":"2009-10-13T16:53:40.309312Z","url":"https://files.pythonhosted.org/packages/f7/87/489f642d16248b974191670c11deb340c9a4a19f51719e70c5b1c09ff269/Products.ContentWellPortlets-2.0.1.tar.gz","yanked":false,"yanked_reason":null}],"2.0dev":[{"comment_text":"","digests":{"blake2b_256":"241530522abc5b920f6f7f8aee20751d59b6895eedeef5eb021cafc0d385c65f","md5":"a45ebd84f0a46b7628d639451b8db80a","sha256":"3d28a86b54169fee64b1d67e761891cc2d5a7a0e5ee2bd904b92705ba002bbe8"},"downloads":-1,"filename":"Products.ContentWellPortlets-2.0dev-py2.4.egg","has_sig":false,"md5_digest":"a45ebd84f0a46b7628d639451b8db80a","packagetype":"bdist_egg","python_version":"2.4","requires_python":null,"size":24251,"upload_time":"2009-10-13T16:47:54","upload_time_iso_8601":"2009-10-13T16:47:54.009566Z","url":"https://files.pythonhosted.org/packages/24/15/30522abc5b920f6f7f8aee20751d59b6895eedeef5eb021cafc0d385c65f/Products.ContentWellPortlets-2.0dev-py2.4.egg","yanked":false,"yanked_reason":null},{"comment_text":"","digests":{"blake2b_256":"407f1f8a5b0ad551b6ee5a97049fc716e169c62674761f03df3a1f4d9bf57251","md5":"8772873b4c5bd439ae36eb8e1e085a53","sha256":"350e53631fa448461d7a8bb0db74a4ff7b64612ec27d11eb6a49e0aff686b4a0"},"downloads":-1,"filename":"Products.ContentWellPortlets-2.0dev.tar.gz","has_sig":false,"md5_digest":"8772873b4c5bd439ae36eb8e1e085a53","packagetype":"sdist","python_version":"source","requires_python":null,"size":18652,"upload_time":"2009-10-13T16:47:53","upload_time_iso_8601":"200