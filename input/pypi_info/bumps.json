umps-0.7.15.tar.gz","yanked":false,"yanked_reason":null}],"0.7.16":[{"comment_text":"","digests":{"blake2b_256":"08eca0f67cabd2c1b354382030c88b50b123b68c0bb8784726cae9171d6dead9","md5":"d0c88dc252479a2bb19ccb66e9a65705","sha256":"3594452487b8404f1efaace9b70aefaeb345fa44dd74349f7829a61161d2f69a"},"downloads":-1,"filename":"bumps-0.7.16.tar.gz","has_sig":false,"md5_digest":"d0c88dc252479a2bb19ccb66e9a65705","packagetype":"sdist","python_version":"source","requires_python":null,"size":515824,"upload_time":"2020-06-11T04:19:02","upload_time_iso_8601":"2020-06-11T04:19:02.454665Z","url":"https://files.pythonhosted.org/packages/08/ec/a0f67cabd2c1b354382030c88b50b123b68c0bb8784726cae9171d6dead9/bumps-0.7.16.tar.gz","yanked":false,"yanked_reason":null}],"0.7.17":[{"comment_text":"","digests":{"blake2b_256":"939985dda3d5153e6fc28320a7f16186fb3a9019b3399595ccd2858cea01af69","md5":"c6ca6f060c65d4b2d7b0af678a7d537b","sha256":"29ddb6c6704b6bb7bc59704b976579f11a150bf7068c140d006525036f1eef09"},"downloads":-1,"filename":"bumps-0.7.17.tar.gz","has_sig":false,"md5_digest":"c6ca6f060c65d4b2d7b0af678a7d537b","packagetype":"sdist","python_version":"source","requires_python":null,"size":522570,"upload_time":"2020-11-07T00:35:16","upload_time_iso_8601":"2020-11-07T00:35:16.612463Z","url":"https://files.pythonhosted.org/packages/93/99/85dda3d5153e6fc28320a7f16186fb3a9019b3399595ccd2858cea01af69/bumps-0.7.17.tar.gz","yanked":false,"yanked_reason":null}],"0.7.18":[{"comment_text":"","digests":{"blake2b_256":"71c68cc6b8dc8be765e8ea3ccd7b905d176d17a3c048f74bc7551f802c2ae63e","md5":"2ff16b20b0ec094b56ee5f977d25e0cc","sha256":"3217d4fd3ec767448d742f3b6ff527cc3817f2421b9a9a8456e1d8ee4a9b1087"},"downloads":-1,"filename":"bumps-0.7.18.tar.gz","has_sig":false,"md5_digest":"2ff16b20b0ec094b56ee5f977d25e0cc","packagetype":"sdist","python_version":"source","requires_python":null,"size":522596,"upload_time":"2020-11-16T15:42:50","upload_time_iso_8601":"2020-11-16T15:42:50.033148Z","url":"https://files.