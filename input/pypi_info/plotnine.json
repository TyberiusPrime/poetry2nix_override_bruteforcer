","build; extra == \"build\"","wheel; extra == \"build\"","twine; extra == \"dev\"","plotnine[typing]; extra == \"dev\"","pre-commit; extra == \"dev\"","jupyter; extra == \"doc\"","nbsphinx; extra == \"doc\"","click; extra == \"doc\"","numpydoc>=0.9.1; extra == \"doc\"","quartodoc>=0.7.2; extra == \"doc\"","importlib-resources; extra == \"doc\"","adjustText; extra == \"extra\"","geopandas; extra == \"extra\"","scikit-learn; extra == \"extra\"","scikit-misc>=0.3.0; extra == \"extra\"","ruff; extra == \"lint\"","pytest-cov; extra == \"test\"","pyright==1.1.362; extra == \"typing\"","ipython; extra == \"typing\"","pandas-stubs; extra == \"typing\""],"requires_python":">=3.9","summary":"A Grammar of Graphics for Python","version":"0.13.6","yanked":false,"yanked_reason":null},"last_serial":23137550,"releases":{"0.1.0":[{"comment_text":"","digests":{"blake2b_256":"fd271b3f63322cb80f0db49e6ff71845446466347075912b0f31e176f8d37fc9","md5":"39dd3466360f89f0bb2829464df28781","sha256":"ea89159dd94c3cd1149ae5464eab81187c31ffbff977df30c14300e42736e563"},"downloads":-1,"filename":"plotnine-0.1.0-py2.py3-none-any.whl","has_sig":false,"md5_digest":"39dd3466360f89f0bb2829464df28781","packagetype":"bdist_wheel","python_version":"py2.py3","requires_python":null,"size":3166055,"upload_time":"2017-04-25T20:37:59","upload_time_iso_8601":"2017-04-25T20:37:59.480246Z","url":"https://files.pythonhosted.org/packages/fd/27/1b3f63322cb80f0db49e6ff71845446466347075912b0f31e176f8d37fc9/plotnine-0.1.0-py2.py3-none-any.whl","yanked":false,"yanked_reason":null},{"comment_text":"","digests":{"blake2b_256":"4593851ce9c23d0565e65f4fb69319a837078ab7e26a71ff9100d775df6d3e07","md5":"55d87ffd54da1a7ed436241159393b3e","sha256":"ee0e4c55f373fb84433d50bcc6acf1b5eab1a05fd5e21378ebdcc7306453aa76"},"downloads":-1,"filename":"plotnine-0.1.0.tar.gz","has_sig":false,"md5_digest":"55d87ffd54da1a7ed436241159393b3e","packagetype":"sdist","python_version":"source","requires_python":null,"size":2995199,"upload_time":"201